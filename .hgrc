[paths]
default = ssh://hg@bitbucket.org/mforbes/mmfutils-fork
pristine = ssh://hg@bitbucket.org/mforbes/mmfutils

[alias]
qcom = commit

[hooks]
#pre-qcom = jupyter nbconvert --to=rst --output=README.rst doc/README.ipynb
